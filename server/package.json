{
  "name": "what-the-flag",
  "version": "0.1.0",
  "description": "A Quizduell-like game with questions about countries, capitals and flags.",
  "private": true,
  "main": "server.js",
  "type": "module",
  "engines": {
    "node": "20"
  },
  "scripts": {
    "start": "node server.js",
    "server": "nodemon -r dotenv/config server",
    "client": "npm start --prefix client",
    "build": "npm run build --prefix client",
    "heroku-postbuild": "npm i -f --prefix client && npm run build --prefix client",
    "create-challenge-of-the-week": "node -r dotenv/config ./utils/createChallengeOfTheWeek.js"
  },
  "keywords": [
    "quiz",
    "game",
    "countries"
  ],
  "author": "Nico Gr√§f",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "compression": "^1.7.4",
    "country-quiz": "^0.5.2",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.1"
  },
  "devDependencies": {
    "dotenv": "^16.3.1",
    "nodemon": "^3.0.1",
    "prettier": "^3.1.0"
  }
}
